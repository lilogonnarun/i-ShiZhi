}
.Card_List .card .mid .topic{

}
.Card_List .card .mid .time{

}
.Card_List .card .fee{

}
.Card_List .card .mid .specialty_list .star{
  width: 30rpx;
  height: 30rpx;
  border-radius: 100%;
  
  will-change: transform;
}

.Card_List .card .mid .specialty_list .star image{
  width: 100%;
  height: 100%;
  border-radius: 100%;
  will-change: transform;
}
.Card_List .card .head .head_name .name {
  text-size-adjust: 20rpx;
  font-weight: 600;
  font-size: 30rpx;
}
.Card_List .card .head .background {
  font-size: smaller;
  color: #4d4d4d;
  display:table-column-group;
}
.Card_List .card .specialty {
  margin-left: 20rpx;
  line-height: 1.5;
  font-size: 28rpx;
}